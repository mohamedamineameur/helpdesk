{
    "name": "helpdesk-server",
    "version": "1.0.0",
    "description": "Backend Helpdesk Fullstack - Express.js, TypeScript, Sequelize, SQLite3",
    "main": "src/server.ts",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc",
        "start": "node build/server.js",
        "lint": "eslint ./src --ext .ts",
        "prettier": "prettier --write ./src",
        "test": "NODE_ENV=test jest --config jest.config.ts",
        "create:admin": "ts-node scripts/create-admin.ts"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^4.18.2",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "sequelize": "^6.37.1",
        "sequelize-typescript": "^2.1.4",
        "sqlite3": "^5.1.6"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.21",
        "@types/helmet": "^4.0.0",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/morgan": "^1.9.4",
        "@types/node": "^20.8.10",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "jest": "^29.7.0",
        "prettier": "^3.2.5",
        "supertest": "^6.3.3",
        "ts-jest": "^29.3.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.2"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}
